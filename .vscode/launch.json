  "configurations": [
    {
      // Launch npm fullStart
      "type": "node",
      "request": "launch",
      "name": "Launch npm fullStart",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run-script", "fullStart"]
    },
    {
      // Launch npm start
      "type": "node",
      "request": "launch",
      "name": "Launch npm start",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run-script", "start"]
    },
    {
      // Launch startDatabase
      "type": "node",
      "request": "launch",
      "name": "Launch startDatabase",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run-script", "startDatabase"]
    },
    {
      // Launch startDatabaseDebug
      "type": "node",
      "request": "launch",
      "name": "Launch startDatabaseDebug",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run-script", "startDatabaseDebug"]
    }
  ]
